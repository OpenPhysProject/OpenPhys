<div class="rlo-content">
    <div class="rlo-content-title">300.7 Quantum Mechanics</div>
    <div class="rlo-content-body">
        
        <figure>
    <img class="img-responsive rlo-content-image" src="/images/RLO1/Wave.svg" />
    <figcaption></figcaption>
    </figure>
        
    </div>
    
    <canvas id="myCanvas" width="600" height="100" style="border:1px solid #c3c3c3;">
        Sorry.
    </canvas>    
<script>
    //Create a stage by getting a reference to the canvas
    stage = new createjs.Stage("myCanvas");
    //Create a Shape DisplayObject.
    circle = new createjs.Shape();
    circle.graphics.beginFill("red").drawCircle(0, 0, 40);
    //Set position of Shape instance.
    circle.x = circle.y = 50;
    //Add Shape instance to stage display list.
    stage.addChild(circle);
    //Update stage will render next frame
    stage.update();
 

    //Update stage will render next frame
    createjs.Ticker.addEventListener("tick", handleTick);

    function handleTick() {
     //Circle will move 10 units to the right.
        circle.x += 10;
        if (circle.x > stage.canvas.width/2) 
            { circle.graphics.beginFill("green").drawCircle(0, 0, 40); }
        //Will cause the circle to wrap back
        if (circle.x > stage.canvas.width) { circle.x = 0; }
        stage.update();
        
     circle.addEventListener("click", handleClick);
 function handleClick(event){
     // Click happenened
          circle.graphics.beginFill("blue").drawCircle(0, 0, 40);
 }    
        
    }
</script>    
    
 
</div>
